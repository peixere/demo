            "type": "basicfunction",
            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "fn": "showform",
                "designer|params": [
                    "record"
                ],
                "implHandler": [
                    "var me = this;\r",
                    "if (record !== null && record !== '')\r",
                    "{\r",
                    "    var formwin = Ext.create('Gotom.view.RightWindow');\r",
                    "    formwin.addListener('close', function(panel,opts)\r",
                    "    {\r",
                    "        me.loadRight();\r",
                    "    });\r",
                    "    formwin.show();\r",
                    "    formwin.form.getForm().reset();\r",
                    "    formwin.form.loadRecord(record);\r",
                    "}\r",
                    "else\r",
                    "{\r",
                    "    Ext.Msg.show(\r",
                    "    {\r",
                    "        // width : 200\r",
                    "        title : \"操作提示\",\r",
