            "codeClass": null,
            "userConfig": {
                "fn": "saveForm",
                "implHandler": [
                    "var me = this;\r",
                    "if (Ext.getCmp('UserForm').isValid())\r",
                    "{\r",
                    "    var userId = Ext.getCmp('user.id').getValue();\r",
                    "    var name = Ext.getCmp('user.name').getValue();                \r",
                    "    var username = Ext.getCmp('user.username').getValue();\r",
                    "    var pkIds = [];\r",
                    "    var tree = Ext.getCmp('RoleTreePanel');\r",
                    "    var items = tree.getSelectionModel().store.data.items;\r",
                    "    Ext.each(items, function()\r",
                    "    {\r",
                    "        var nd = this;\r",
                    "        if(nd.data.checked)\r",
                    "        {\r",
                    "            pkIds.push(nd.data.id);\r",
                    "        }\r",
                    "    });\r",
                    "    var wait = Ext.Msg.wait(\"正在加载......\", \"操作提示\");\r",
                    "    Ext.Ajax.request(\r",
                    "    {\r",
                    "        url : 'User!save.do',\r",
                    "        method : 'POST',\r",
                    "        params:{\r",
                    "            'user.id':userId,\r",
                    "            'user.name':name,\r",
                    "            'user.username':username,\r",
                    "            'roleIds':pkIds\r",
                    "        },  \r",
                    "        success : function(response, options)\r",
                    "        {\r",
                    "            wait.close();\r",
                    "            var result = Ext.JSON.decode(response.responseText); \r",
                    "            if(result.success)\r",
                    "            {\r",
                    "                me.loadGrid();\r",
                    "                me.loadFormData('');\r",
                    "            }\r",
                    "            else\r",
                    "            {\r",
                    "                Ext.Msg.alert('信息提示', result.data);\r",
                    "            }\r",
                    "        },\r",
                    "        failure : function(response, options)\r",
                    "        {\r",
                    "            wait.close();\r",
                    "            if(response.status == 200)\r",
                    "            {\r",
                    "                var result = Ext.JSON.decode(response.responseText);\r",
                    "                Ext.Msg.alert('信息提示', result.data);\r",
                    "            }\r",
                    "            else\r",
                    "            {\r",
                    "                Ext.Msg.alert('信息提示', response.responseText);\r",
                    "            }\r",
                    "        }\r",
                    "    });\r",
                    "}"
                ]
            },
            "designerId": "e6f1a3c5-bee7-47e6-b050-b67a75aa22d7"
