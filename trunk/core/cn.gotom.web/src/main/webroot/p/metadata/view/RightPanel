                ],
                "implHandler": [
                    "var me = this;\r",
                    "var selected = Ext.getCmp('RightTreePanel').getSelectionModel().selected;\r",
                    "var p = selected.items[0];\r",
                    "var parentId = '';\r",
                    "if (!Ext.isEmpty(p, false))\r",
                    "{\r",
                    "    parentId = p.data.id;\r",
                    "    if (p.data.leaf)\r",
                    "    {\r",
                    "        Ext.Msg.alert(\"操作提示\", \"请选择目录类型节点！\");\r",
                    "        return;\r",
                    "    }\r",
                    "}\r",
                    "var wait = Ext.Msg.wait(\"正在执行......\", \"操作提示\");\r",
                    "Ext.Ajax.request(\r",
                    "{\r",
                    "    url : ctxp + '/p/right!fresh.do',\r",
                    "    method : 'POST',\r",
                    "    success : function(response, options)\r",
                    "    {\r",
                    "        var result = Ext.JSON.decode(response.responseText);\r",
                    "        var data = Ext.create('Gotom.model.RightTreeCheckModel');\r",
                    "        result.parentId = parentId;\r",
                    "        data.data = result;\r",
                    "        data.data.leaf = leaf;\r",
                    "        if(leaf){\r",
                    "            data.data.type = 'URL';\r",
                    "        }else{\r",
                    "            data.data.type = 'DIR';\r",
                    "        }\r",
                    "        wait.close();\r",
                    "        me.showform(data);\r",
                    "    },\r",
                    "    failure : function(response, options)\r",
                    "    {\r",
                    "        wait.close();\r",
                    "        Ext.Msg.alert(\"操作提示\", \"操作失败\");\r",
                    "    }\r",
